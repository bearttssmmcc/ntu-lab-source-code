{
  "name": "tag",
  "description": "Migrate from canary to prod triggered by creation of any tag",
  "filename": "build/tag-cloudbuild.yaml",
  
  "triggerTemplate": {
    "projectId": "ntu-hands-on-lab",
    "repoName": "gke-progression",
    "tagName": ".*"
  },
  "substitutions": {
    "_ZONE": "us-central1-b",
    "_CLUSTER": "gke-progression-cluster"
  }

}